<?xml version="1.0" encoding="euc-kr"?>
<config>
	<!--<defender> 배포 금지여부 체크
		path=경로, extension=확장자, useDeploy=배포여부(false 배포금지)
	-->
	<repository name='DINLE_UI_LIB/dinle/nxui' type='defender'>
		<defender path='' extension='JSON' useDeploy='false'/>
		<defender path='' extension='JS' useDeploy='false'/>
		<defender path='/nexacro14lib' extension='JSON' useDeploy='false'/>
		<defender path='/nexacro14lib' extension='INFO' useDeploy='false'/>
		<defender path='/nexacro14lib/component/ComComp' extension='!JS' useDeploy='false'/>
		<defender path='/nexacro14lib/framework' extension='!JS' useDeploy='false'/>
		<defender path='/nexacro14lib/component/CompBase' extension='!JS' useDeploy='false'/>
		<defender path='/nexacro14lib/component/DesignComp' extension='*' useDeploy='false'/>
		<defender path='*' extension='JSP' useDeploy='false'/>
	</repository>

	<!-- <convert> nxui_src 빌드후 JS추가 변환여부 체크
		path=경로, extension=확장자, useConvert=JS변환여부(false 변환안함)
		-->
	<repository name='DINLE_UI_LIB/dinle/nxui' type='convert'>
		<convert path='/font' extension='*' useConvert='false'/>
		<convert path='/images' extension='*' useConvert='false'/>
		<convert path='/_theme_' extension='*' useConvert='false'/>
		<convert path='/web' extension='JS' useConvert='false'/>
		<convert path='/web' extension='HTML' useConvert='false'/>
		<convert path='' extension='HTML' useConvert='false'/>
	</repository>

	<!-- 넥사크로 설치 파일  JS추가 변환여부 체크 -->
	<repository name='DINLE_UI_LIB/install_nexacro' type='convert'>
		<convert path='' extension='*' useConvert='false'/>
	</repository>

	<!-- 업무에서 배포되어야 하는 리스트 -->
	<domain name='ACT' type='pattern' desc=''>
		<pattern path='/act' extension='JS'/>
	</domain>
	<domain name='ATS' type='pattern'>
		<pattern path='/ats' extension='JS'/>
	</domain>
	<domain name='CUS' type='pattern'>
		<pattern path='/cus' extension='JS'/>
	</domain>
	<domain name='GNI' type='pattern'>
		<pattern path='/gni' extension='JS'/>
	</domain>
	<domain name='LTI' type='pattern'>
		<pattern path='/lti' extension='JS'/>
	</domain>
	<domain name='MTI' type='pattern'>
		<pattern path='/mti' extension='JS'/>
	</domain>
	<domain name='ONC' type='pattern'>
		<pattern path='/onc' extension='JS'/>
	</domain>
	<domain name='PDF' type='pattern'>
		<pattern path='/pdf' extension='JS'/>
	</domain>
	<domain name='SAL' type='pattern'>
		<pattern path='/sal' extension='JS'/>
	</domain>
	<domain name='STA' type='pattern'>
		<pattern path='/sta' extension='JS'/>
	</domain>
	<domain name='TCM' type='pattern'>
		<pattern path='' extension='HTML'/>
		<pattern path='' extension='JS'/>
		<pattern path='/_theme_' extension='JS'/>
		<pattern path='/_theme_' extension='PNG'/>
		<pattern path='/bizComm' extension='JS'/>
		<pattern path='/comm' extension='JS'/>
		<pattern path='/css' extension='JS'/>
		<pattern path='/font' extension='CSS'/>
		<pattern path='/font' extension='TTF'/>
		<pattern path='/frame' extension='JS'/>
		<pattern path='/guide' extension='JS'/>
		<pattern path='/images' extension='PNG'/>
		<pattern path='/images' extension='GIF'/>
		<pattern path='/images' extension='JPG'/>
		<pattern path='/images' extension='JPEG'/>
		<pattern path='/lib' extension='JS'/>
		<pattern path='/main' extension='JS'/>
		<pattern path='/nexacro14lib' extension='JS'/>
		<pattern path='/sample' extension='JS'/>
		<pattern path='/template' extension='JS'/>
		<pattern path='/template' extension='GIF'/>
		<pattern path='/web' extension='JS'/>
		<pattern path='/web' extension='HTML'/>
	</domain>

	<domain name='UWT' type='pattern'>
		<pattern>/uwt/*.JS</pattern>
	</domain>

	<!-- 미사용 -->
	<repository name='DINLE_UI_LIB/dinle/tcm' type='defender'>
		<defender path='/zcs' extension='*'/>
	</repository>
	<!-- 미사용 -->
	<repository name='DINLE_UI_LIB/dinle' type='defender'>
		<defender path='' extension='*' file='dinle_c_ci*' useDeploy='false'/>
	</repository>
</config>
